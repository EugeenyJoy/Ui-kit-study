doctype html
html
head
    
    block head
.wrapper.formElements
    .subLogo
            include ../..//assets/img/subLogo.svg
    .row
        .col
            //- Text Field
            .element_container
                .elemTitle_container
                    h4 Text Field
                    .elemSubtitle Default
                .element
                include ../includes/modules/formElements/textField.pug

            //- Text Field hover/focus
            .element_container
                .elemTitle_container
                    h4 Text Field
                    .elemSubtitle Hover / Focus
                .element
                include ../includes/modules/formElements/textField_HovFoc.pug

            //- Dropdown
            .element_container
                .elemTitle_container
                    h4 Dropdown
                    .elemSubtitle Default
                .element
                include ../includes/modules/formElements/Dropdown.pug

            div(style="display:flex;")
                //- date_dropdown
                .element_container
                    .elemTitle_container
                        h4 date Dropdown
                    .element
                    include ../includes/modules/formElements/date_dropdown.pug

                //- dateNum_dropdown
                .element_container(style="margin-left: 20px;")
                    .elemTitle_container
                        h4 date Dropdown
                    .element
                    include ../includes/modules/formElements/dateNum_dropdown.pug

            //- masked Text Field
            .element_container
                .elemTitle_container
                    h4 Masked Text Field
                    .elemSubtitle Default
                .element
                include ../includes/modules/formElements/maskedTextField.pug

            //- filter date dropdown
            .element_container
                .elemTitle_container
                    h4 filter date dropdown
                .element
                include ../includes/modules/formElements/filter_date_dropdown.pug

            //- subscription text field
            .element_container
                .elemTitle_container
                    h4 subscription text field
                include ../includes/modules/formElements/subscriptionTextField.pug

        .separate40
    
        .col
            //- Checkbox Buttons
            .element_container
                .elemTitle_container(style="margin-bottom:15px;")
                    h4 Checkbox Buttons
                .element
                include ../includes/modules/formElements/checkbox_buttons.pug

            .separate40

             //- Radio Buttons
            .element_container
                .elemTitle_container(style="margin-bottom:15px;")
                    h4 Radio Buttons
                .element
                include ../includes/modules/formElements/radio_buttons.pug

            .separate40

             //- Toggle
            .element_container
                .elemTitle_container(style="margin-bottom:15px;")
                    h4 Toggle
                .element
                include ../includes/modules/formElements/toggle_buttons.pug
            
            .separate40

             //- Like buttons
            .element_container
                .elemTitle_container(style="margin-bottom:15px;")
                    h4 Like Button
                .element
                //- include ../includes/modules/formElements/like_buttons.pug

        .col

    .row
        .col
        .col
        .col
        
    .row
        .col
        .col


   
     
            

block script
    script(type="text/javascript")